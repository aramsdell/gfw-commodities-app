.gfw #app-body #homeView {
	height auto !important
}

.gfw #splashDialog {
    background url('images/landing.png')
    border 1px solid #888
}

.gfw #splashDialog a {
    color: #97bd3d
}

.gfw #splashDialog label {
    padding 0 10px 15px
    float right
}

.gfw #splashDialog .dijitDialogTitleBar {
    background none
}

.gfw #splashDialog .dijitDialogPaneContent {
    background none
    font-size 15px
}

.gfw #app-body .background-container {
	overflow hidden
	height 100%
	width 100%
    min-height 520px
}

.gfw #app-body .background-container img {
	height: auto;
    position: absolute;
	width: 100%;
}

.homeBgContainer {
	width 100%
    height 100%
    overflow hidden
}

.homeBgContainer img {
    width 100%
    height auto
}

.homeModeDotsContainer {
    z-index: 200;
    position: absolute;
    align: center;
    right: 5%;
    bottom 8%
}

.homeModeOptionsContainer {
    position absolute
    z-index 200
    width 100%
    height 100%
}

.homeModeOptionsViewerOuter {
    width 75%
    height 300px
    z-index 100
    //overflow hidden
    overflow-x auto
    overflow-y hidden
    margin auto
    margin-top 9em
    overflow visible
}

.homeModeOuter {
    z-index 100
    //overflow hidden
    overflow-x auto
    overflow-y hidden
    margin auto
    overflow visible
    text-align center
    transition: all 0.25s
}

.homeModeDot {
    height 15px
    width 15px
    margin 5px 10px
    display inline-block
    background-color white
    transition all 2s
    border-radius 10px
    cursor pointer
}

.homeModeDot[title]:hover:after {
  content: attr(title);
  padding: 4px 8px;
  color: #E98300;
  position: absolute;
  right: 0;
  top: 100%;
  z-index: 20;
  white-space: nowrap;
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  border-radius: 0;
  -moz-box-shadow: 0px 0px 4px #222;
  -webkit-box-shadow: 0px 0px 4px #222;
  box-shadow: 0px 0px 4px #222;
  //background-image: -moz-linear-gradient(top, #eeeeee, #cccccc);
  //background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, #eeeeee),color-stop(1, #cccccc));
  //background-image: -webkit-linear-gradient(top, #eeeeee, #cccccc);
  //background-image: -moz-linear-gradient(top, #eeeeee, #cccccc);
  //background-image: -ms-linear-gradient(top, #eeeeee, #cccccc);
  //background-image: -o-linear-gradient(top, #eeeeee, #cccccc);
    background-color #fff;
    border 1px solid separatorGray

}

.homeModeDot.activeDot {
    background-color #fff
    transition all 2s
    //transition display 1.5s ease
    opacity: 1;
    transform: scale(1.2)
}

.homeModeOptionsViewerOuter .modeGroup {
    height 300px
    width 100%
    transition display 1.5s ease
    cursor pointer
    font-size 24px
}


.homeModeOptionsViewerOuter .modeOption {
    border-radius 150px
    //background-color #000
