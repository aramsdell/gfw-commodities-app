.homeModeOptionsViewerOuter .modeGroup .modeText span {
    font-size 31px
    font-family: 'Fira Sans Ultra Light', 'fira_sans_otregular', Georgia, serif;
}

.homeModeOptionsViewerOuter .modeGroup .modeText span.more-text {
    position relative
    top -1px
}
.homeModeOptionsViewerOuter .modeGroup .modeText span.less-text {
    position relative
    top 10px
}

#signUpAlertsForm {
    font-family "Arial MT Condensed Light"
    src url("../fonts/ArialMTStd-LightCond.otf") format("opentype")
    color #464052
}

#signUpAlertsForm .select-container {
    background url('../images/ico-carat-down.png') no-repeat 95% center transparent
    width 240px
}

#signUpAlertsForm #aoiDistrictPicker {
    background transparent
    -webkit-appearance none
    -moz-appearance none
    appearance none
    font-size 14px
    padding 5px
    height 30px
    width 240px
}

#signUpAlertsForm #aoiSubDistrictPicker {
    border 1px solid #BBB
    border-radius 5px
    font-size 12px
    width 240px
}

#signUpAlertsForm input[type="email"], #signUpAlertsForm input[type="tel"] {
    border-radius 5px
    font-size 16px
    width 235px
    height 30px
}

#signUpAlertsForm #verifyEmailForAlerts {
    font-size 12px
    display none
}

#alerts-submit-button {
    background #e50036
    padding 5px 10px
    cursor pointer
    font-size 14px
    color white
    border none
}

#signUpAlertsForm td {
    padding 2px 6px
}

#signUpAlertsForm .error-messages {
	color #e50036
}

.image-box {
    width: 200px;
    height: 200px;
    position: absolute;
    right: 0;
    background: red;
}
.image-box img {
    width 200px
    height 200px
}
