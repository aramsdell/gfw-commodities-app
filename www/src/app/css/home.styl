.home-slider-container {
    width: 400px;
    text-align: left;
}

.home-slider-container h3 {
    font-size 2.3em
}
.home-slider-container h4 {
    margin 0
}
.home-slider-container p {
    font-size 31px
    float: left
    clear: both
}
.home-slider-container a {
    font-size: 0.6em;
    font-family: 'Fira Sans', 'fira_sans_otregular', Georgia, serif;
    text-transform: uppercase;
    padding: 0.5em 1.5em;
    background: #5a5a5a;
    color: #fff !important;
    clear: both
}
.home-slider-container a:hover {
    background-color orangeSelected
}
.home-slider-container h3,
.home-slider-container span,
.home-slider-container p {
    font-family: 'Fira Sans', 'fira_sans_otregular', Georgia, serif;
    text-transform: uppercase;
    line-height 1em
    color #555
    text-align left
    margin 0
    display: inline-block
    padding: 3px 10px;
    background: rgba(255,255,255,0.75);
}

.homeModeOptionsViewerOuter .modeGroup .modeText {
    font-family "Georgia",serif
    color #CCC
    position absolute
    left 0px
    right 0px
    vertical-align middle
}

#homeView > div > div.homeModeOptionsContainer > div > div:nth-child(1) > div.modeOptionRing > div > span {
    color: black !important;
}
#homeView > div > div.homeModeOptionsContainer > div > div:nth-child(1) > div.modeOptionRing {
    background-image: url("../../app/css/images/backgroundCircle.png");
}
#homeView > div > div.homeModeOptionsContainer > div > div:nth-child(3) > div.modeOptionRing > div > span {
    color: black !important;
}
