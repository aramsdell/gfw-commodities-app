.gfw .app-header .filter-list-title
	text-align center
	margin 15px auto
	display block

.gfw .app-header .filter-list
	font-family 'Fira Sans', 'fira_sans_otregular', Georgia, serif
	list-style-type none
	font-size 13px
	padding 0
	margin 0

.gfw .app-header .filter-list .layer-sub-title
	margin 3px 3px 3px 15px
	font-size 10px

.gfw .app-header .filter-list .layer-title,
.gfw .app-header .filter-list .layer-sub-title
	color mediumGray

.gfw .app-header .filter-list .layer-title
	margin-left 5px

.gfw .filter-list .layer-list-item
	border-bottom 1px solid baseLightGray
	cursor pointer
	padding 2px 5px

.gfw .filter-list .layer-list-item.hidden
	display none

.gfw .layer-list-item input[type='radio']
	margin 3px 0

