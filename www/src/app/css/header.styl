	list-style-type none
	margin-left 20px
	font-size 13px
	padding 0

.gfw .app-header .filter-list ul > li
	border none
	
.info-icon-svg
	pointer-events none
	height 9px
	width 10px
	margin auto

.gfw .app-header .layer-info-icon
.wizard-checkbox .layer-info-icon
.suitability-accordion .layer-info-icon
	float right
	display flex
	background-color #ddd
	height 15px
	width 15px
	color #888 !important
	border-radius 50%
	
.gfw .app-header .layer-info-icon:hover
.wizard-checkbox .layer-info-icon:hover
.suitability-accordion .layer-info-icon:hover
	background-color #888

